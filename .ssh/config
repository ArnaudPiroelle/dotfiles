# Lan
## Pi Nodes
Host tardis.pi
    hostname tardis.pi
    port 22
    identityfile ~/.ssh/keys/lan/id_rsa
Host doctor.pi
    hostname doctor.pi
    port 22
    identityfile ~/.ssh/keys/lan/id_rsa
Host amy.pi
    hostname amy.pi
    port 22
    identityfile ~/.ssh/keys/lan/id_rsa

## Tech Nodes
Host nas.tech
    hostname nas.tech
    user root
    port 22
    identityfile ~/.ssh/keys/lan/id_rsa

# Git Repositories
Host github.com
    hostname github.com
    user git
    port 22
    identityfile ~/.ssh/keys/github/id_rsa
Host bitbucket.org
    hostname bitbucket.org
    user git
    port 22
    identityfile ~/.ssh/keys/bitbucket/id_rsa

